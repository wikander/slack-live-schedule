{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "amd": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 1,
    "quotes": [1, "single" ],
    "no-undef": 1,
    "strict": [1, "global"],
    "no-extra-semi": 1,
    "no-underscore-dangle": 1,
    "no-console": 0,
    "no-unused-vars": 1,
    "no-trailing-spaces": 1,
    "no-unreachable": 1,
    "no-alert": 1
  }
}
